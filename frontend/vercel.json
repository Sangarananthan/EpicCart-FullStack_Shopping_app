{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://epic-cart.onrender.com"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://epic-cart-wheat.vercel.app"
        },
        { "key": "Access-Control-Allow-Credentials", "value": "true" }
      ]
    }
  ]
}
